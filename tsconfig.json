{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "rootDir": "./src/",
    "outDir": "./build/",
    "strict": true,
    "noImplicitAny": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
    "docs"
  ],
  "include": [
    "src"
  ]
}
